{
    "liveSassCompile.settings.formats": [
        {
            "format": "expanded",
            "extensionName": ".css",
            "savePath": "/css/"
        },
        {
            "format": "compressed",
            "extensionName": ".min.css",
            "savePath": "/assets/css/"
        }
